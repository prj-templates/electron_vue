{
  "name": "electron_vue",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"cross-env NODE_ENV=development vite\" \"wait-on tcp:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "vue-tsc -b && vite build && electron-builder",
    "electron:preview": "vite preview"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "electron-squirrel-startup": "^1.0.1",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^2.3.0",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vue-tsc": "^2.2.0",
    "wait-on": "^8.0.1"
  }
}